{
  "name": "vue-simple-docs",
  "version": "1.0.0",
  "description": "Create your project's documentation in few simple steps.",
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "generate:data": "node ./dist/deleteGeneratedData.js && node ./dist/generateData.js",
    "generate": "node ./dist/index.js && npm run generate:data",
    "format": "npx prettier --write .",
    "prepare": "husky install"
  },
  "bin": {
    "delete-generated": "./dist/deleteGeneratedData.js",
    "generate-data": "./dist/generateData.js",
    "generate": "./dist/index.js"
  },
  "keywords": [
    "vue",
    "documentation",
    "docs",
    "generate"
  ],
  "author": "Willo0ow",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./package.json": "./package.json"
  },
  "types": "./dist/lib/types.ts",
  "files": [
    "dist",
    "lib",
    "template",
    "package.json",
    "README.md"
  ],
  "devDependencies": {
    "@types/node": "^18.15.11",
    "@types/prettier": "^2.7.2",
    "husky": "^8.0.0",
    "pretty-quick": "^3.1.3",
    "tsup": "^6.7.0",
    "typescript": "^5.0.3"
  },
  "dependencies": {
    "@types/fs-extra": "^11.0.1",
    "cosmiconfig": "^8.1.3",
    "fs-extra": "^11.1.1",
    "prettier": "^2.8.4"
  }
}
